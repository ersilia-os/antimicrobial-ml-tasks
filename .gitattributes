data/efaecium/efaecium_binary.csv filter=lfs diff=lfs merge=lfs -text
data/enterobacter/enterobacter_binary.csv filter=lfs diff=lfs merge=lfs -text
data/kpneumoniae/kpneumoniae_binary.csv filter=lfs diff=lfs merge=lfs -text
